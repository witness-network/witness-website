{
  "name": "witness-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "react-scroll": "^1.5.4"
  },
  "scripts": {
    "build-css": "node-sass --output-style compressed src/css/app.scss src/app.css",
    "watch-css": "node-sass --watch --output-style compressed src/css/app.scss src/app.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
